{
  "name": "@guivic/fabric-node",
  "version": "0.0.20",
  "description": "node.js helpers to create quickly micro services and API",
  "main": "index.js",
  "scripts": {
    "cover": "nyc --reporter=html --all npm test",
    "coveralls": "nyc --reporter=lcovonly mocha && cat ./coverage/lcov.info | coveralls",
    "test": "mocha test/test.config.js test/*.test.js test/**/*.test.js --exit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "commit": "git-cz"
  },
  "engines": {
    "node": ">=8"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guivic/fabric-node.git"
  },
  "keywords": [
    "guivic",
    "guivic-fabric-node",
    "fabric-node"
  ],
  "author": "lludol",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guivic/fabric-node/issues"
  },
  "homepage": "https://github.com/guivic/fabric-node#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^2.9.6",
    "coveralls": "^3.0.2",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^4.16.0",
    "eslint-config-lludol": "^0.1.7",
    "eslint-plugin-chai-expect": "^1.1.1",
    "eslint-plugin-mocha": "^5.2.0",
    "mocha": "^5.2.0",
    "nyc": "^13.0.1"
  },
  "dependencies": {
    "@google-cloud/storage": "^5.8.2",
    "apollo-server-koa": "^1.3.6",
    "apollo-upload-server": "^5.0.0",
    "cloudinary": "^1.11.0",
    "fs-extra": "^6.0.1",
    "graphql": "^0.13.2",
    "graphql-apollo-errors": "^2.0.3",
    "graphql-tools": "^3.0.2",
    "joi": "^13.1.1",
    "koa": "^2.5.0",
    "koa-better-body": "^3.0.4",
    "koa-body": "^4.0.1",
    "koa-bodyparser": "^4.2.0",
    "koa-datadog-middleware": "^1.2.0",
    "koa-jwt": "^3.3.2",
    "koa-morgan": "^1.0.1",
    "koa-mount": "^2.0.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa2-cors": "^2.0.6",
    "koa2-validation": "^0.1.0",
    "nodemailer": "^4.4.2",
    "nodemailer-mandrill-transport": "^1.2.0",
    "nodemailer-sendgrid": "^1.0.2",
    "raven": "^2.4.0",
    "winston-lludol": "^0.0.5"
  },
  "nyc": {
    "exclude": [
      "example",
      "test",
      "coverage"
    ]
  }
}
